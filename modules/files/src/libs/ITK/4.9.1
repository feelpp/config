#%Module1.0#####################################################################
###
### ITK 4.9.1
###
proc ModulesHelp { } {
    global version prefix

    puts stderr "\tlibs/ITK/4.9.1 - loads ITK 4.9.1 and its environment"
}

module-whatis   "Loads ITK 4.9.1 and its environment"

# for Tcl script use only
set version     4.9.1
set prefix      $::env(FEELPP_ITK491_PATH)

prepend-path CMAKE_PREFIX_PATH $prefix
prepend-path PATH $prefix/bin
prepend-path LD_LIBRARY_PATH $prefix/lib
prepend-path PYTHONPATH $prefix/lib/paraview-5.0/site-packages
setenv ITK_DIR $prefix
