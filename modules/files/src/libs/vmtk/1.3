#%Module1.0#####################################################################
###
### vmtk 1.3 module
###
proc ModulesHelp { } {
    global version prefix

    puts stderr "\tlibs/vmtk/1.3 - loads vmtk 1.3"
}

module-whatis   "Loads vmtk 1.3"

# for Tcl script use only
set version     1.3
set prefix      $::env(FEELPP_VMTK13_PATH)

prepend-path CMAKE_PREFIX_PATH $prefix
prepend-path PATH $prefix/bin
prepend-path LD_LIBRARY_PATH $prefix/lib/
prepend-path PYTHONPATH $prefix/lib/python2.7/site-packages:$prefix/lib/python2.7/site-packages/vtk:$prefix/lib/python2.7/site-packages/vmtk
