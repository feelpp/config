#%Module1.0################
set allineapath $::env(FEELPP_ALLINEA_PATH)

set allineabinpath $allineapath/bin
set allinealibpath $allineapath/lib/64

if [module-info mode load]  {
	prepend-path PATH $allineabinpath
	prepend-path LD_LIBRARY_PATH $allinealibpath
    prepend-path CMAKE_PREFIX_PATH $allineapath
}

if [module-info mode remove] {
    remove-path PATH $allineabinpath
    remove-path LD_LIBRARY_PATH $allinealibpath
    remove-path CMAKE_PREFIX_PATH $allineapath
}
