#%Module1.0################
set meshgemspath $::env(FEELPP_MESHGEMS20_PATH)

set meshgemsbinpath $meshgemspath/bin
set meshgemslibpath $meshgemspath/lib/Linux_64

if [module-info mode load]  {
	prepend-path PATH $meshgemsbinpath
	prepend-path LD_LIBRARY_PATH $meshgemslibpath
}

if [module-info mode remove] {
    remove-path PATH $meshgemsbinpath
    remove-path LD_LIBRARY_PATH $meshgemslibpath
}
