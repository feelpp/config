#%Module1.0#####################################################################
###
### ParaView 4.3.1
###
proc ModulesHelp { } {
    global version prefix

    puts stderr "\ttools/ParaView/4.3.1 - loads ParaView 4.3.1 and its environment"
}

module-whatis   "Loads ParaView 4.3.1 and its environment"

# for Tcl script use only
set version     4.3.1
set prefix      $::env(FEELPP_PARAVIEW431_PATH)

prepend-path CMAKE_PREFIX_PATH $prefix
prepend-path PATH $prefix/bin
prepend-path LD_LIBRARY_PATH $prefix/lib
prepend-path LD_LIBRARY_PATH $prefix/lib/paraview-4.3
prepend-path PYTHONPATH $prefix/lib/paraview-4.3/site-packages
